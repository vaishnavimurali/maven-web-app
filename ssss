pipeline{
    agents any
    tools{
        maven 'Maven_3_5_0'

    }
    stages{
        stage('Build Maven'){
            steps{
                checkout scmGit(branches: [[name: '*/main']], extensions: [], userRemoteConfigs: [[url: 'https://github.com/vaishnavimurali/devops-automation']])
                sh 'mvn clean install'

            }
        }
    }
}







